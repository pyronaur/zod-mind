{
	"name": "zod-mind",
	"license": "MIT",
	"version": "0.2.2",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/pyronaur/zod-mind.git"
	},
	"scripts": {
		"build": "tsup index.ts --format cjs,esm --dts",
		"dev": "tsup index.ts --format cjs,esm --dts --watch --ignore-watch dist",
		"release": "pnpm run build && changeset version && changeset publish",
		"lint": "tsc",
		"publish": "changeset && changeset version"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@types/node": "^20.2.1",
		"tsup": "^6.6.2",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"zod": "^3.21.4",
		"zod-to-json-schema": "^3.21.1"
	}
}
